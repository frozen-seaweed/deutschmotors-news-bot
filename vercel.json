{
  "functions": {
    "api/*.py": {
      "runtime": "python3.x"
    }
  },
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py"
    },
    {
      "src": "/api/stats", 
      "dest": "/api/webhook.py"
    },
    {
      "src": "/",
      "dest": "/api/webhook.py"
    }
  ]
}
